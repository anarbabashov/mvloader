# YouTube Bypass Configuration
PROXY_HOST=your-proxy-server.com
PROXY_PORT=8080
PROXY_USERNAME=username
PROXY_PASSWORD=password

# Alternative: Use rotating proxy service
ROTATING_PROXY_URL=http://username:password@proxy-provider.com:8080

# YouTube API Fallback (optional)
YOUTUBE_API_KEY=your-youtube-api-key

# Rate limiting
MAX_REQUESTS_PER_HOUR=100
ENABLE_RATE_LIMITING=true